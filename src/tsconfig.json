{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "declaration": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../dist/",
        "sourceMap": false
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./AbstractElement.ts",
        "./ConnectorElement.ts",
        "./cubitt-graph.ts",
        "./EdgeElement.ts",
        "./ElementType.ts",
        "./Graph.ts",
        "./GraphInterface.ts",
        "./ModelElement.ts",
        "./NodeElement.ts",
        "./Project.ts",
        "./test/deleteGraphTest.ts",
        "./test/deserializeGraphTest.ts",
        "./test/InsertGraphTest.ts",
        "./test/triangleGraphDeleteGraphTest.ts",
        "./typings/assertion-error/assertion-error.d.ts",
        "./typings/chai/chai.d.ts",
        "./typings/mocha/mocha.d.ts",
        "./typings/tsd.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
